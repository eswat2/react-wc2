{
  "name": "react-wc2",
  "description": "This project is created using Direflow",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf build dist lib",
    "start": "direflow-scripts start",
    "build": "direflow-scripts build",
    "build:lib": "direflow-scripts build:lib",
    "dist": "devolution build dist",
    "dev": "yarn build && yarn dist",
    "test": "direflow-scripts test"
  },
  "dependencies": {
    "devolution": "2.3.1",
    "direflow-component": "3.5.2",
    "direflow-scripts": "3.5.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-scripts": "4.0.2"
  },
  "devDependencies": {
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-react": "7.22.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "react-app-rewired": "2.1.8",
    "react-test-renderer": "17.0.1",
    "to-string-loader": "1.1.6",
    "webpack-cli": "4.5.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config-overrides-path": "direflow-webpack.js"
}
